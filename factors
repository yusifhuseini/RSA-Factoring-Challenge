#!/usr/bin/python3

# factors
import sys

def factorize(number):
    for divisor in range(2, number):
        if number % divisor == 0:
            return divisor, number // divisor

if len(sys.argv) != 2:
    print("Usage: factors <file>")
    sys.exit(1)

input_file = sys.argv[1]

with open(input_file, 'r') as file:
    for line in file:
        number = int(line)
        p, q = factorize(number)
        print(f"{number}={p}*{q}")
